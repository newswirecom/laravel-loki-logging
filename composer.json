{
    "name": "newswirecom/laravel-loki-logging",
    "version": "0.0.1",
    "description": "Logging to Loki for Laravel",
    "type": "library",
    "authors": [
        {
            "name": "Newswire Development Team",
            "email": "devops@newsire.com"
        }
    ],
    "require": {
        "php": "^7.0|^8.0",
        "ext-json": "*",
        "illuminate/support": "^8.0.0|^9.0.0",
        "illuminate/console": "^8.0.0|^9.0.0",
        "monolog/monolog": "^2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Newswirecom\\LaravelLokiLogging\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Newswirecom\\LaravelLokiLogging\\L3ServiceProvider"
            ]
        }
    }
}
